		{
			"name": "Update booking",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"// Pre-request Script to generate and set a new token\r",
							"pm.sendRequest({\r",
							"    url: \"https://restful-booker.herokuapp.com/auth\",\r",
							"    method: \"POST\",\r",
							"    header: { \"Content-Type\": \"application/json\" },\r",
							"    body: {\r",
							"        mode: \"raw\",\r",
							"        raw: JSON.stringify({\r",
							"            username: \"admin\",\r",
							"            password: \"password123\"\r",
							"        })\r",
							"    }\r",
							"}, function (err, res) {\r",
							"    if (err) {\r",
							"        console.error(\"Auth request failed:\", err);\r",
							"    } else {\r",
							"        let data;\r",
							"        try {\r",
							"            data = res.json();\r",
							"        } catch (e) {\r",
							"            data = {};\r",
							"        }\r",
							"        if (data && data.token) {\r",
							"            pm.environment.set(\"rb_token\", data.token);\r",
							"            console.log(\"New token set:\", data.token);\r",
							"        } else {\r",
							"            console.warn(\"No token returned. Response:\", res.text());\r",
							"        }\r",
							"    }\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
