		{
			"name": "Delete booking",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"// === STEP 1: Create a fresh token ===\r",
							"pm.sendRequest({\r",
							"    url: \"https://restful-booker.herokuapp.com/auth\",\r",
							"    method: \"POST\",\r",
							"    header: { \"Content-Type\": \"application/json\" },\r",
							"    body: {\r",
							"        mode: \"raw\",\r",
							"        raw: JSON.stringify({\r",
							"            username: \"admin\",\r",
							"            password: \"password123\"\r",
							"        })\r",
							"    }\r",
							"}, function (err, res) {\r",
							"    if (err) {\r",
							"        console.error(\"Auth request failed:\", err);\r",
							"        return;\r",
							"    }\r",
							"\r",
							"    let data = {};\r",
							"    try { data = res.json(); } catch (e) {}\r",
							"    if (data && data.token) {\r",
							"        pm.environment.set(\"rb_token\", data.token);\r",
							"        console.log(\"New token saved:\", data.token);\r",
							"\r",
							"        // === STEP 2: Create a new booking ===\r",
							"        const payload = {\r",
							"            firstname: \"Auto\",\r",
							"            lastname: \"Delete\",\r",
							"            totalprice: 199,\r",
							"            depositpaid: true,\r",
							"            bookingdates: {\r",
							"                checkin: \"2025-09-01\",\r",
							"                checkout: \"2025-09-05\"\r",
							"            },\r",
							"            additionalneeds: \"NA\"\r",
							"        };\r",
							"\r",
							"        pm.sendRequest({\r",
							"            url: \"https://restful-booker.herokuapp.com/booking\",\r",
							"            method: \"POST\",\r",
							"            header: {\r",
							"                \"Content-Type\": \"application/json\",\r",
							"                \"Accept\": \"application/json\"\r",
							"            },\r",
							"            body: {\r",
							"                mode: \"raw\",\r",
							"                raw: JSON.stringify(payload)\r",
							"            }\r",
							"        }, function (err2, res2) {\r",
							"            if (err2) {\r",
							"                console.error(\"Booking create failed:\", err2);\r",
							"                return;\r",
							"            }\r",
							"\r",
							"            let book = {};\r",
							"            try { book = res2.json(); } catch (e) {}\r",
							"            if (book && book.bookingid) {\r",
							"                pm.environment.set(\"bookingid\", book.bookingid);\r",
							"                console.log(\"New bookingid saved:\", book.bookingid);\r",
							"            } else {\r",
							"                console.error(\"No bookingid returned:\", res2.text());\r",
							"            }\r",
							"        });\r",
							"    } else {\r",
							"        console.error(\"No token returned:\", res.text());\r",
							"    }\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {}
					}
