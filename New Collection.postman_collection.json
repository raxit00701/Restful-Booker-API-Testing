							"pm.test(\"Response is JSON\", () => {\r",
							"  pm.expect(pm.response.headers.get(\"Content-Type\")).to.include(\"application/json\");\r",
							"});\r",
							"\r",
							"pm.test(\"Response is an array with bookingid objects\", () => {\r",
							"  const json = pm.response.json();\r",
							"  pm.expect(Array.isArray(json)).to.eql(true, \"Response should be an array\");\r",
							"  pm.expect(json.length).to.be.above(0, \"Array should not be empty\");\r",
							"  // check first N items for shape\r",
							"  const sample = json.slice(0, Math.min(5, json.length));\r",
							"  sample.forEach((item, i) => {\r",
							"    pm.expect(item).to.be.an(\"object\", `Item ${i} should be object`);\r",
							"    pm.expect(item).to.have.property(\"bookingid\");\r",
							"    pm.expect(item.bookingid).to.be.a(\"number\");\r",
							"  });\r",
